{
  "name": "themes",
  "version": "1.0.0",
  "main": "index.js",
  
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "stylelint \"**/*.scss\"",
    "watch": "sass jonatan2/scss/style.scss:jonatan2/css/style.css jonatan2/scss/style-dark.scss:jonatan2/css/style-dark.css --no-source-map --watch",
    "watch-min": "sass jonatan2/scss/style.scss:jonatan2/css/style.min.css jonatan2/scss/style-dark.scss:jonatan2/css/style-dark.min.css --no-source-map --watch --style compressed",
    "style": "npm run style-dark && npm run style-light",
    "style-min": "npm run style-dark-min && npm run style-light-min",
    "style-dark": "sass jonatan2/scss/style-dark.scss jonatan2/css/style-dark.css --no-source-map",
    "style-dark-min": "sass jonatan2/scss/style-dark.scss jonatan2/css/style-dark.min.css --no-source-map --style compressed",
    "style-light": "sass jonatan2/scss/style.scss jonatan2/css/style.css --no-source-map",
    "style-light-min": "sass jonatan2/scss/style.scss jonatan2/css/style.min.css --no-source-map --style compressed"
  },
  
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "normalize.css": "^8.0.1",
    "sass": "^1.29.0",
    "stylelint": "^13.7.2",
    "stylelint-config-sass-guidelines": "^7.1.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1"
  },
  "description": ""
}
